<div id="previewHome" style="display: none">
    <div class='tnd-animated-background banner-preview-home' style="width: 100%">
    </div>
    <div class='tnd-animated-background tnd-mx-auto-xs' style="height: 35px;width: 20%;margin-top: 1.25rem;">
    </div>
    <div class="container" style="margin-top: 1.25rem;">
        <div class="flex-sm">
            {% for _ in 1..5 %}
                <div {% if loop.index0 %} class="tnd-visible-sm tnd-animated-background tnd-m-10-xs" {% else %} class="tnd-animated-background tnd-m-10-xs" {% endif %} style="height: 180px; flex-basis: 20%"></div>
            {% endfor %}
        </div>
        {% for _ in 1..4 %}
            <div class='tnd-animated-background tnd-mx-auto-xs' style="height: 35px;width: 50%;margin-top: 1.25rem;">
            </div>
            <div class="flex-sm flex-cont-3-sm" style="margin-top: 1.25rem;">
                {% for _ in 1..3 %}
                    <div {% if loop.index0 %} class="tnd-visible-sm f4 tnd-animated-background tnd-m-10-xs" {% else %} class="tnd-m-10-xs tnd-animated-background" {% endif %} style="height: 282px;"></div>
                {% endfor %}
            </div>
        {% endfor %}
    </div>
</div>